{
  "port": 9000,
  "endpointConfig": {
    "healthCheck": "/health",
    "cache": "/cache"
  },
  "networkBufferPool": {
    "poolSize": 50,
    "fillOnCreation": true,
    "bufferSize": 1024
  },
  "httpRequestMemoryPool": {
    "poolSize": 50,
    "fillOnCreation": true,
    "bufferSize": 128
  },
  "incomingCacheRequestPool": {
    "poolSize": 50,
    "fillOnCreation": true,
    "bufferSize": 256
  },
  "threadIdleStrategy": {
    "maxSpins": 10,
    "maxYields" : 10,
    "minParkPeriodNs": 10,
    "maxParkPeriodNs" : 1000
  },
  "caches": [
    {
      "name": "example1",
      "maximumKeys": 64,
      "fixedValueSize": 128,
      "valueStorageMechanism": "DirectMemoryStorage",
      "evictionStrategy": "LRU"
    }
  ]
}